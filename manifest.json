{
  "name": "YT Studio hider",
  "description": "Extension that hides youtube studio ranking and icons",
  "manifest_version": 3,
  "version": "0.1",

  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://studio.youtube.com/channel/*",
      ]
    }
  ]
}
